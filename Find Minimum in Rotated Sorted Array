class Solution {
public:
    int findMin(vector<int>& nums) {
        int n =nums.size();
        int i;
        int min=0;
        for(i=0;i<n;i++){
            if (nums[i]<nums[i+1]){
                int min=nums[i];
            }
            else{
                int min=nums[i+1];
            }
        }
        return min;
        
    }
};
